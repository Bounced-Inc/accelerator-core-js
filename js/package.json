{
  "name": "opentok-accelerator-core",
  "version": "0.0.1",
  "description": "",
  "main": "dist/core.js",
  "scripts": {
    "build:node": "babel -d ./dist/ ./src/",
    "build:browser": "browserify src/core.js -o browser/opentok-acc-core.js -t [ babelify --presets [ es2015 ] ] --im",
    "build:react-sample": "cp dist/* react-sample-app/src/ot-core/",
    "build:js-sample": "cp browser/opentok-acc-core.js vanilla-js-sample-app/public/js/components/opentok-acc-core.js",
    "build": "npm run build:node && npm run build:browser && npm run build:react-sample && npm run build:js-sample"
  },
  "keywords": [
    "opentok",
    "opentok-acc-pack",
    "webrtc",
    "communication",
    "video",
    "audio"
  ],
  "author": "adrice727@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "babel-preset-es2015": "^6.16.0",
    "babelify": "^7.3.0",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.2.2",
    "opentok-solutions-logging": "^1.0.6"
  }
}
